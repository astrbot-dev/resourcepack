{
    "model": {
        "type": "composite",
        "models": [
            {
                "type": "select",
                "property": "display_context",
                "fallback": {
                    "type": "model",
                    "model": "astrbot:bot/ground"
                },
                "cases": [
                    {
                        "when": "fixed",
                        "model": {
                            "type": "condition",
                            "property": "custom_model_data",
                            "index": 0,
                            "on_false": {
                                "type": "model",
                                "model": "astrbot:bot/ground"
                            },
                            "on_true": {
                                "type": "model",
                                "model": "astrbot:bot/ground_move"
                            }
                        }
                    }
                ]
            },
            {
                "type": "model",
                "model": "astrbot:modules/ground_right/protection"
            },
            {
                "type": "model",
                "model": "astrbot:modules/ground_left/protection"
            },
            {
                "type": "model",
                "model": "astrbot:modules/ground_middle/protection"
            }
        ]
    }
}